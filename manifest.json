{
  "manifest_version": 3,
  "name": "Focus Tracker",
  "version": "1.0",
  "permissions": [
    "tabs",
    "notifications"
  ],
  
  "background": {
    "service_worker": "background.js"
  },

  "description": "Block out any unwanted distractions",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}